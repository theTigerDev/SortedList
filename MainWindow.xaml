<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedL"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="SortedL.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="23" Margin="175,72,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="23" Margin="175,100,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="175,128,0,0" VerticalAlignment="Top" Width="120" Height="34" Click="btnAdd_Click"/>
        <Button x:Name="btnShow" Content="ShowAll" HorizontalAlignment="Left" Margin="175,167,0,0" VerticalAlignment="Top" Width="120" Height="34" Click="btnShow_Click"/>
        <ListBox x:Name="sortedListBox" HorizontalAlignment="Left" Height="129" Margin="318,72,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
